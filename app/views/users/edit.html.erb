<h1>Rails Edit Form Project</h1>
  <div>
    <%= form.label :username, "name:" %>
    <%= form.text_field :username %>
  </div>
  <div>
    <%= form.label :email, "email:" %>
    <%= form.email_field :email %>
  </div>
  <div>
    <%= form.label :password, "password:" %>
    <%= form.password_field :password %>
  </div>
  <div>
    <%= form.submit "Update" %>
  </div>
<% end %>
